{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, must-revalidate"
          }
        ]
      }
    ],
  "redirects": [
    {
      "source": "/blog/:path*",
      "destination": "/article/:path*"
    }
  ]
}